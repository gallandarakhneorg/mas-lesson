@INPROCEEDINGS{ulieru00fuzzy,
  author = {Ulieru, M. and Stefanoiu, D. and Norrie, D.},
  title = {Holonic self-organization of multi-agent systems by fuzzy modeling
	with application to intelligent manufacturing},
  booktitle = {Systems, Man, and Cybernetics, 2000 IEEE International Conference
	on},
  year = {2000},
  volume = {3},
  pages = {1661-1666},
  abstract = {Holonic manufacturing aims to design standardized, modular manufacturing
	systems made of interchangeable parts, to enable flexibility, online
	reconfigurability and self-organizing capabilities for the production
	systems. Recent advances in distributed artificial intelligence and
	networking technologies have proven that theoretical multi-agent
	systems (MAS) concepts are very suitable for the real life implementation
	of holonic concepts. Building on our recent results in the design
	and implementation of holonic reconfigurable architectures, the paper
	introduces a novel approach to the online self-organization of distributed
	systems. By using fuzzy set and uncertainty theoretical concepts,
	we construct a mathematical foundation for modeling MAS, where appropriate
	holonic structures are identified for each particular application.
